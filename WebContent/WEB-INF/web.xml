  </servlet>
  <servlet-mapping>
    <servlet-name>ReportServlet</servlet-name>
    <url-pattern>/ReportServlet</url-pattern>
  </servlet-mapping>
  
    <servlet>
      <description>更改更人信息</description>
    <display-name>DeleteDetailServlet</display-name>

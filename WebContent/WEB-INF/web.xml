    <servlet-name>AddTypeServlet</servlet-name>
    <servlet-class>com.servlet.AddTypeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddTypeServlet</servlet-name>
    <url-pattern>/AddTypeServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>处理用户添加明细</description>
    <display-name>AddDetailServlet</display-name>
    <servlet-name>AddDetailServlet</servlet-name>
    <servlet-class>com.servlet.AddDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddDetailServlet</servlet-name>
    <url-pattern>/AddDetailServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>更改钱包</description>
